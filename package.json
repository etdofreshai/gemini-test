{
  "name": "gemini-image-gen",
  "version": "2.0.0",
  "description": "Gemini image generation via web API",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n api,client -c blue,green \"tsx watch src/server/index.ts\" \"vite\"",
    "build": "tsc -p tsconfig.server.json && vite build",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mime-types": "^3.0.2",
    "multer": "^1.4.5-lts.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.4",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
}
